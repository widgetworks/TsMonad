{
  "name": "@wiwo/tsmonad",
  "version": "2.0.0",
  "description": "TsMonad - fun-size monads library for TypeScript",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "lib/*",
    "LICENSE-MIT",
    "README.md"
  ],
  "directories": {},
  "scripts": {
    "clean": "rm -rf dist lib",
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.json",
    "build:test": "tsc -p tsconfig.test.json",
    "xxbuild": "webpack lib/src/index.js dist/tsmonad.js -p --devtool source-map --output-library-target umd",
    "prepublish": "npm run build",
    "pretest": "npm run clean && npm run build:test",
    "test": "mocha dist/test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/widgetworks/TsMonad.git"
  },
  "keywords": [
    "typescript",
    "monad",
    "functional"
  ],
  "author": {
    "name": "Chris Bowdon",
    "url": "https://github.com/cbowdon"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^13.9.0",
    "@types/underscore": "^1.8.0",
    "assert": "^1.4.1",
    "mocha": "^7.1.0",
    "source-map-support": "^0.5.16",
    "typescript": "^3.7.2",
    "underscore": "^1.8.3",
    "webpack": "^1.13.1"
  }
}
